[user]
  name = yisselda-bbr
  email = yrhoc@bbrpartners.com

[init]
  defaultBranch = main

[alias]
  ci = commit
  co = checkout
  br = branch
  st = status
  poh = push --set-upstream origin HEAD
  ll = log --oneline
  cia = commit --amend --no-edit
  dmb = !git branch --merged | grep -Ev "(^\*|^\+|master|main|dev)" | xargs --no-run-if-empty git branch -d
